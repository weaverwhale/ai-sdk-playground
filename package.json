{
  "name": "ai-sdk-playground",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "tsx watch --inspect server/server.ts",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "d": "npm run dev",
    "build:frontend": "vite build",
    "build:backend": "tsc -p tsconfig.node.json",
    "build": "npm run clean && npm run build:backend && npm run build:frontend",
    "lint": "eslint .",
    "backend:serve": "tsx dist/backend/server.js",
    "prod": "npm run build && npm run backend:serve",
    "clean": "rm -rf dist",
    "start": "npm run build && npm run backend:serve"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.3",
    "@ai-sdk/cerebras": "^1.0.7",
    "@ai-sdk/deepseek": "^1.0.7",
    "@ai-sdk/google": "^2.0.6",
    "@ai-sdk/google-vertex": "^3.0.7",
    "@ai-sdk/groq": "^2.0.7",
    "@ai-sdk/openai": "^2.0.12",
    "@ai-sdk/react": "^2.0.12",
    "@google/genai": "^1.13.0",
    "@xenova/transformers": "^2.17.2",
    "ai": "^5.0.12",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.3",
    "hnswlib-node": "^3.0.0",
    "mermaid": "^11.5.0",
    "node-localstorage": "^3.0.5",
    "openator": "^1.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-jsx-parser": "^2.4.0",
    "react-markdown": "^10.1.0",
    "react-mermaid-js": "^0.0.1",
    "react-router": "^7.6.0",
    "regression": "^2.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "uuid": "^11.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^24.2.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/regression": "^2.0.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
