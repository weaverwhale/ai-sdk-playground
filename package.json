{
  "name": "ai-sdk-playground",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "client": "vite",
    "server": "tsx server/server.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:seq": "npm run server & sleep 2 && npm run client",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "npm run dev:seq",
    "start:client": "npm run client",
    "start:server": "npm run server"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.2.6",
    "@ai-sdk/react": "^1.1.24",
    "ai": "^4.1.63",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^18.19.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "tsx": "^4.7.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
